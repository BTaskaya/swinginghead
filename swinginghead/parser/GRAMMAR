start: (expr)*
?expr: pointer | literal | funcdecl

funcdecl: "swing" typedecl "$" typedecl? ("â‚¬" typedecl)* "$"

pointer: "<-" literal
literal: "(" typedecl "->" (ATOM | seq) ")"
typedecl: "`" CNAME ("<" meta ("," meta)* ">")? "`"
meta: SIGNED_NUMBER | typedecl

seq: "@" ATOM? ("~" ATOM)* "@"
ATOM: SIGNED_NUMBER | "void"
%import common.CNAME
%import common.SIGNED_NUMBER
%import common.WS
%ignore WS
